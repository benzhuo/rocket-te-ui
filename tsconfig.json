
{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom"
    ],
    "paths": {
      "rocket-ui-lib": [
        "dist/rocket-ui-lib"
      ],
	    "rocket-ui-lib/*": [
        "dist/rocket-ui-lib/*"
      ]
    },
    "module": "esnext",
    "baseUrl": "./"
  },
  "angularCompilerOptions": {
		"enableIvy": false
	}
}